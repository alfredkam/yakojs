{
  "name": "yako",
  "version": "0.3.29",
  "description": "A minimal and light weight graph",
  "repository": {
    "type": "git",
    "url": "https://github.com/alfredkam/yakojs.git"
  },
  "scripts": {
    "test": "./node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha test -- --recursive",
    "coveralls": "./node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha test -- --recursive --report loconly -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot",
    "pack": "webpack --config ./build-tools/demo.config.js -p && webpack --config ./build-tools/example.config.js -p && webpack --config ./build-tools/expose.config.js && webpack --config ./build-tools/expose.min.config.js -p && webpack --config ./build-tools/addons.expose.config.js && webpack --config ./build-tools/addons.expose.min.config.js -p"
  },
  "keywords": [
    "graph",
    "graph library",
    "graph api",
    "beautiful graph",
    "minimal graph",
    "serverside rendering",
    "chart",
    "react"
  ],
  "engines": {
    "node": ">=0.8.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/alfredkam/yakojs/blob/master/LICENSE.md"
    }
  ],
  "author": "Alfred Kam",
  "readmeFilename": "README.md",
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel-core": "^5.2.17",
    "babel-loader": "^5.0.0",
    "chai": "^2.0.0",
    "coveralls": "^2.11.2",
    "es6-loader": "^0.2.0",
    "expose-loader": "^0.6.0",
    "file-loader": "^0.8.1",
    "gulp": "^3.8.11",
    "gulp-load-plugins": "^0.10.0",
    "gulp-nodemon": "^2.0.3",
    "gulp-plumber": "^1.0.0",
    "gulp-webpack": "^1.4.0",
    "istanbul": "^0.3.5",
    "jsx-loader": "^0.12.2",
    "jymin": "^0.4.1",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "lodash": "^3.5.0",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.1",
    "react": "^0.12.2",
    "react-hot-loader": "^1.2.3",
    "style-loader": "^0.8.3",
    "webpack": "^1.5.3",
    "webpack-dev-middleware": "^1.0.11",
    "webpack-dev-server": "^1.7.0"
  }
}
